/-!
### Factorial
-/


/-- The series `‚àë' n, x ^ n / n!` is summable of any `x : ‚Ñù`. See also `exp_series_div_summable`
for a version that also works in `‚ÑÇ`, and `exp_series_summable'` for a version that works in
any normed algebra over `‚Ñù` or `‚ÑÇ`. -/
theorem Real.summable_pow_div_factorial (x : ‚Ñù) : Summable (fun n => x ^ n / n ! : ‚Ñï ‚Üí ‚Ñù) :=
  by
  -- We start with trivial extimates
  have A : (0 : ‚Ñù) < ‚åä‚Äñx‚Äñ‚åã‚Çä + 1 := zero_lt_one.trans_le (by simp)
  have B : ‚Äñx‚Äñ / (‚åä‚Äñx‚Äñ‚åã‚Çä + 1) < 1 := (div_lt_one A).2 (Nat.lt_floor_add_one _)
  -- Then we apply the ratio test. The estimate works for `n ‚â• ‚åä‚Äñx‚Äñ‚åã‚Çä`.
  suffices : ‚àÄ n ‚â• ‚åä‚Äñx‚Äñ‚åã‚Çä, ‚Äñx ^ (n + 1) / (n + 1)!‚Äñ ‚â§ ‚Äñx‚Äñ / (‚åä‚Äñx‚Äñ‚åã‚Çä + 1) * ‚Äñx ^ n / ‚Üën !‚Äñ
  exact summable_of_ratio_norm_eventually_le B (eventually_at_top.2 ‚ü®‚åä‚Äñx‚Äñ‚åã‚Çä, this‚ü©)
  -- Finally, we prove the upper estimate
  intro n hn
  calc
    ‚Äñx ^ (n + 1) / (n + 1)!‚Äñ = ‚Äñx‚Äñ / (n + 1) * ‚Äñx ^ n / n !‚Äñ := by
      rw [pow_succ, Nat.factorial_succ, Nat.cast_mul, ‚Üê div_mul_div_comm, norm_mul, norm_div,
        Real.norm_coe_nat, Nat.cast_succ]
    _ ‚â§ ‚Äñx‚Äñ / (‚åä‚Äñx‚Äñ‚åã‚Çä + 1) * ‚Äñx ^ n / n !‚Äñ := by
      mono* with 0 ‚â§ ‚Äñx ^ n / n !‚Äñ, 0 ‚â§ ‚Äñx‚Äñ <;> apply norm_nonneg
    
#align real.summable_pow_div_factorial Real.summable_pow_div_factorial

theorem Real.tendsto_pow_div_factorial_at_top (x : ‚Ñù) :
    Tendsto (fun n => x ^ n / n ! : ‚Ñï ‚Üí ‚Ñù) atTop (ùìù 0) :=
  (Real.summable_pow_div_factorial x).tendsto_at_top_zero
#align real.tendsto_pow_div_factorial_at_top Real.tendsto_pow_div_factorial_at_top

