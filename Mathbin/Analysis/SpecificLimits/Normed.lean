theorem is_o_pow_pow_of_lt_left {r‚ÇÅ r‚ÇÇ : ‚Ñù} (h‚ÇÅ : 0 ‚â§ r‚ÇÅ) (h‚ÇÇ : r‚ÇÅ < r‚ÇÇ) :
    (fun n : ‚Ñï => r‚ÇÅ ^ n) =o[at_top] fun n => r‚ÇÇ ^ n :=
  have H : 0 < r‚ÇÇ := h‚ÇÅ.trans_lt h‚ÇÇ
  (is_o_of_tendsto fun n hn => False.elim <| H.ne' <| pow_eq_zero hn) <|
    (tendsto_pow_at_top_nhds_0_of_lt_1 (div_nonneg h‚ÇÅ (h‚ÇÅ.trans h‚ÇÇ.le)) ((div_lt_one H).2 h‚ÇÇ)).congr
      fun n => div_pow _ _ _
#align is_o_pow_pow_of_lt_left is_o_pow_pow_of_lt_left

theorem is_O_pow_pow_of_le_left {r‚ÇÅ r‚ÇÇ : ‚Ñù} (h‚ÇÅ : 0 ‚â§ r‚ÇÅ) (h‚ÇÇ : r‚ÇÅ ‚â§ r‚ÇÇ) :
    (fun n : ‚Ñï => r‚ÇÅ ^ n) =O[at_top] fun n => r‚ÇÇ ^ n :=
  h‚ÇÇ.eq_or_lt.elim (fun h => h ‚ñ∏ is_O_refl _ _) fun h => (is_o_pow_pow_of_lt_left h‚ÇÅ h).IsO
#align is_O_pow_pow_of_le_left is_O_pow_pow_of_le_left

theorem is_o_pow_pow_of_abs_lt_left {r‚ÇÅ r‚ÇÇ : ‚Ñù} (h : |r‚ÇÅ| < |r‚ÇÇ|) :
    (fun n : ‚Ñï => r‚ÇÅ ^ n) =o[at_top] fun n => r‚ÇÇ ^ n :=
