  normed_field.tendsto_norm_zpow_nhds_within_0_at_top NormedField.tendsto_norm_zpow_nhds_within_0_at_top

/-- The (scalar) product of a sequence that tends to zero with a bounded one also tends to zero. -/
theorem tendsto_zero_smul_of_tendsto_zero_of_bounded {Œπ ùïú ùî∏ : Type _} [NormedField ùïú]
    [NormedAddCommGroup ùî∏] [NormedSpace ùïú ùî∏] {l : Filter Œπ} {Œµ : Œπ ‚Üí ùïú} {f : Œπ ‚Üí ùî∏}
    (hŒµ : Tendsto Œµ l (ùìù 0)) (hf : Filter.IsBoundedUnder (¬∑ ‚â§ ¬∑) l (norm ‚àò f)) :
    Tendsto (Œµ ‚Ä¢ f) l (ùìù 0) := by
  rw [‚Üê is_o_one_iff ùïú] at hŒµ‚ä¢
  simpa using is_o.smul_is_O hŒµ (hf.is_O_const (one_ne_zero : (1 : ùïú) ‚â† 0))
#align
  normed_field.tendsto_zero_smul_of_tendsto_zero_of_bounded NormedField.tendsto_zero_smul_of_tendsto_zero_of_bounded

@[simp]
theorem continuous_at_zpow {ùïú : Type _} [NontriviallyNormedField ùïú] {m : ‚Ñ§} {x : ùïú} :
    ContinuousAt (fun x => x ^ m) x ‚Üî x ‚â† 0 ‚à® 0 ‚â§ m :=
  by
  refine' ‚ü®_, continuous_at_zpow‚ÇÄ _ _‚ü©
  contrapose!; rintro ‚ü®rfl, hm‚ü© hc
  exact
    not_tendsto_at_top_of_tendsto_nhds (hc.tendsto.mono_left nhds_within_le_nhds).norm
      (tendsto_norm_zpow_nhds_within_0_at_top hm)
#align normed_field.continuous_at_zpow NormedField.continuous_at_zpow

@[simp]
theorem continuous_at_inv {ùïú : Type _} [NontriviallyNormedField ùïú] {x : ùïú} :
    ContinuousAt Inv.inv x ‚Üî x ‚â† 0 := by
  simpa [(zero_lt_one' ‚Ñ§).not_le] using @continuous_at_zpow _ _ (-1) x
#align normed_field.continuous_at_inv NormedField.continuous_at_inv

