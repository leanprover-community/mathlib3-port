  ⟨fun h => ⟨fun a => acc_antisymmetrization_iff.1 <| h.apply _⟩, fun h =>
    ⟨by
      rintro ⟨a⟩
      exact acc_antisymmetrization_iff.2 (h.apply a)⟩⟩
