        (AddCon.ker_rel _).2 <| by
          simp_rw [AddMonoidHom.map_add, FreeAddMonoid.lift_eval_of, f.map_add₂]
      | _, _, eqv.of_add_right m n₁ n₂ =>
        (AddCon.ker_rel _).2 <| by
          simp_rw [AddMonoidHom.map_add, FreeAddMonoid.lift_eval_of, (f m).map_add]
