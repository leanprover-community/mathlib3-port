        (AddCon.ker_rel _).2 <| by
          simp_rw [AddMonoidHom.map_zero, FreeAddMonoid.lift_eval_of, f.map_zero₂]
      | _, _, eqv.of_zero_right m =>
        (AddCon.ker_rel _).2 <| by
          simp_rw [AddMonoidHom.map_zero, FreeAddMonoid.lift_eval_of, (f m).map_zero]
