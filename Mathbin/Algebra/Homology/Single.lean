def single₀ : V ⥤ ChainComplex V ℕ
    where
  obj X :=
    { pt := fun n =>
        match n with
        | 0 => X
        | n + 1 => 0
      d := fun i j => 0 }
  map X Y f :=
    {
      f := fun n =>
        match n with
        | 0 => f
        | n + 1 => 0 }
  map_id' X := by
    ext n
    cases n
    rfl
    dsimp
    unfold_aux
    simp
  map_comp' X Y Z f g := by
    ext n
    cases n
    rfl
    dsimp
    unfold_aux
    simp
