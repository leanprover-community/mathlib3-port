    obviously
  -- unitality
  left_unitality' : ‚àÄ X : C, (Œª_ (obj X)).Hom = (Œµ ‚äó ùüô (obj X)) ‚â´ Œº (ùüô_ C) X ‚â´ map (Œª_ X).Hom := by
    obviously
  right_unitality' : ‚àÄ X : C, (œÅ_ (obj X)).Hom = (ùüô (obj X) ‚äó Œµ) ‚â´ Œº X (ùüô_ C) ‚â´ map (œÅ_ X).Hom := by
