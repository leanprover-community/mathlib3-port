  toFun : α → HahnSeries Γ R
  isPwo_iUnion_support' : Set.IsPwo (⋃ a : α, (to_fun a).support)
  finite_co_support' : ∀ g : Γ, { a | (to_fun a).coeff g ≠ 0 }.Finite
