theorem continuous_toTopMap {x y : SimplexCategory} (f : x ⟶ y) : Continuous (toTopMap f) :=
  Continuous.subtype_mk
    (continuous_pi fun i =>
      continuous_finset_sum _ fun j hj => (continuous_apply _).comp continuous_subtype_val)
